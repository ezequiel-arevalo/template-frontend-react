{
  "name": "template-frontend-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "tsc -b && vite build",
    "type-check": "tsc --noEmit",
    "format": "biome format src/",
    "format:fix": "biome format --write src/",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --write src/",
    "check": "biome check src/",
    "check:fix": "biome check --write src/",
    "test": "vitest",
    "test:run": "vitest run",
    "verify": "bun run type-check && bun run check && bun run test:run",
    "verify:fix": "bun run type-check && bun run check:fix && bun run test:run",
    "ci": "bun run type-check && bun run check && bun run build && bun run test:run"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.1",
    "lefthook": "^2.0.12",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.11.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^27.3.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.16"
  }
}
